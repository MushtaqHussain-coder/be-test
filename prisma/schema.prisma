datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-1.1.x"]
}

model User {
  id    Int     @id @default(autoincrement())
  name  String
  email String  @unique
  jwt   String?
}

model Company {
  id       Int      @id @default(autoincrement())
  name     String
  location String
  url      String
}
